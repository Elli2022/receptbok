{
  "compilerOptions": {
    "baseUrl": ".", // Lägg till denna rad för att ange projektroten som baseUrl
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node", // Ändra från "bundler" till "node"
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "paths": {
      "@/*": [
        "src/*"
      ] // Använd enkel asterisk för att matcha filer i 'src' mappen
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
, "tailwind.config.ts"  ],
  "exclude": [
    "node_modules"
  ]
}
